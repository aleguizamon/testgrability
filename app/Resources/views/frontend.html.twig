{% extends '::base.html.twig' %}
    {% block metas %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="Prueba para Grability por Andres Leguizamon">
        <meta name="author" content="Andres Leguizamon">
    {% endblock %}
    {% block stylesheets %}
        {% stylesheets
        'bundles/app/css/bootstrap.min.css'
        'bundles/app/css/docs.min.css'
        'bundles/app/css/jumbotron-narrow.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock %}

    {% block javascripts %}
        {% javascripts
        'bundles/app/js/jquery-2.1.4.min.js'
        'bundles/app/js/bootstrap.min.js'
        'bundles/app/js/app.js' %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}

    {% block body %}
        <div class="container">

          {% block content %}{% endblock %}

          <footer class="footer">
              <p>Desarrollado por Andr&eacute;s Leguizam&oacute;n</p>
          </footer>

        </div> <!-- /container -->

    {% endblock %}